import matplotlib.pyplot as plt

aar = [2002, 2003, 2004, 2005, 2006, 2007,
           2008, 2009, 2010, 2011, 2012, 2013,
           2014, 2015, 2016, 2017, 2018, 2019]
inntekt = [28.37, 32.19, 36.84, 39.79,
               44.28, 51.12, 60.42, 58.44,
               62.48, 69.94, 73.12, 77.85,
               86, 83, 93.58, 85.32,
               89.95, 110.36, 125.84]

plt.bar(aar, inntekt)
plt.show()